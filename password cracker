from random import *
import os
import time

u_pwd = input("Enter a password: ")
pwd = list("abcdefghijklmnopqrstuvwxyz0123456789")

pw = ""
while pw != u_pwd:
    pw = ""
    for letter in range(len(u_pwd)):
        guess_pwd = pwd[randint(0, len(pwd)-1)]
        pw += guess_pwd
    print("Trying:", pw)
    time.sleep(0.1)   # slows down so you can see progress

print("Your password is:", pw)
